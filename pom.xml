<project>
    <modelVersion>4.0.0</modelVersion>

    <groupId>de.terraconia</groupId>
    <artifactId>terrapaper-parent</artifactId>
    <version>dev-1.16.1-R0.1-SNAPSHOT</version>
    <packaging>pom</packaging>
    <name>TerraPaper (Parent)</name>

    <properties>
        <repo>http://terraconia.de:8081/artifactory</repo>
        <skipTests>true</skipTests>
    </properties>

    <repositories>
        <repository>
            <id>aikar</id>
            <url>https://repo.aikar.co/content/repositories/aikar/</url>
        </repository>
    </repositories>

    <build>
        <defaultGoal>install</defaultGoal>
        <finalName>${project.artifactId}</finalName>
    </build>
    <modules>
        <module>TerraPaper-API</module>
        <module>TerraPaper-Server</module>
    </modules>
</project>
